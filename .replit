modules = ["nodejs-20", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["google-cloud-sdk-gce", "google-cloud-sdk"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Guitar Vault App"

[[workflows.workflow]]
name = "Guitar Vault App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd \"BondStGuitars/GuitarVault\" && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32993
externalPort = 3000

[[ports]]
localPort = 46711
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "start"]
build = ["npm", "run", "build"]
